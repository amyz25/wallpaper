        <ul class="navigation__list">
            <form action="/" class="nform" method="post" enctype="multipart/form-data">

                <div class="nform__group">

                    <select class="nform__input" name="type">
                        <option selected disabled>select type</option>
                        <option value="NATURAL">NATURAL</option>
                        <option value="RAIN">RAIN</option>
                        <option value="FOREST">FOREST</option>
                        <option value="FLOWER">FLOWER</option>
                        <option value="MOUNTAIN">MOUNTAIN</option>
                        <option value="OCEAN">OCEAN</option>
                        <option value="ORCHARD">ORCHARD</option>
                        <option value="CITY">CITY</option>
                        <option value="CARS">CARS</option>
                    </select>


                </div>

                <div class="nform__group">
                    
                    <input type="text" class="nform__input" placeholder="keyword" id="name" name="keyword"  required />
                    <label for="name" class="nform__label">keyword</label>
                    

                </div>

                <div class="nform__group">

                    <div class="nform__input">
                        <div class="nform__group--upload">

                            <input class="nform__upload" type="file" name="image" required>
                            <span>üëÜüèª üëÜüèª üëÜüèª üëÜüèª üëÜüèª</span>
                        </div>
                    </div>
                </div>

                <div class="nform__group">
                    <div class="row">
                        <button class="btn btn--blue btn--nav" type="submit">
                            SAV
                        </button>

                        <button class="btn btn--pink btn--nav" type="reset">
                            RES
                        </button>
                    </div>
                </div>
            </form>
    </nav>
</div>

